{
    "name": "brangerieau/symfonycms",
    "description": "SymfonyCms is a symfony bundle that allows you to quickly create a website with user and content management",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.1",
        "symfony/skeleton": "6.1.*"
    },
    "require-dev": {
        "symfony/webpack-encore-bundle": "^1.15",
        "phpstan/phpstan": "^1.8",
        "friendsofphp/php-cs-fixer": "^3.10",
        "symfony/maker-bundle": "^1.45"
    },
    "license": "GPL-3.0",
    "autoload": {
        "psr-4": {
            "Brangerieau\\SymfonyCmsBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "BRANGERIEAU Thibaud",
            "email": "contact@brangerieau-thibaud.fr"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
