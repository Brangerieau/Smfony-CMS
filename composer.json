{
    "name": "brangerieau/symfonycms",
    "description": "SymfonyCms is a symfony bundle that allows you to quickly create a website with user and content management",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.1",
        "erusev/parsedown": "^1.7",
        "symfonycasts/reset-password-bundle": "^1.14",
        "symfony/security-bundle": "6.1.*",
        "symfony/translation": "6.1.*",
        "symfony/orm-pack": "2.2.0",
        "symfony/mailer": "6.1.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "symfony/twig-bundle": "6.1.*"
    },
    "require-dev": {
        "symfony/webpack-encore-bundle": "^1.15",
        "phpstan/phpstan": "^1.8",
        "friendsofphp/php-cs-fixer": "^3.10"
    },
    "license": "GPL-3.0",
    "autoload": {
        "psr-4": {
            "Brangerieau\\SymfonyCmsBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "BRANGERIEAU Thibaud",
            "email": "contact@brangerieau-thibaud.fr"
        }
    ],
    "minimum-stability": "stable"
}
